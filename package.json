{
  "name": "cozy-store",
  "version": "1.0.5",
  "main": "src/index.jsx",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint && yarn tx",
    "build": "yarn build:browser",
    "build:browser": "cozy-scripts build --browser",
    "build:mobile": "cozy-scripts build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cozy-scripts watch --browser",
    "watch:mobile": "cozy-scripts watch --mobile",
    "standalone": "yarn standalone:browser",
    "standalone:browser": "cozy-scripts standalone --hot --browser",
    "standalone:mobile": "cozy-scripts standalone --hot --mobile",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=https://$GITHUB_TOKEN@github.com/cozy/cozy-store.git",
    "cozyPublish": "git fetch origin ${DEPLOY_BRANCH:-build}:${DEPLOY_BRANCH:-build} && cozy-scripts publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "pretest": "yarn lint",
    "test": "cozy-scripts test --verbose --coverage",
    "stack:docker": "docker run --rm -it -p 8080:8080 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-store.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-store/issues"
  },
  "homepage": "https://github.com/cozy/cozy-store#readme",
  "devDependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-cozy-app": "0.10.0",
    "cozy-scripts": "0.10.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-cozy-app": "0.10.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "git-directory-deploy": "1.5.1",
    "identity-obj-proxy": "3.0.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-markdown": "3.3.4",
    "react-test-renderer": "16.4.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "sinon": "6.1.3",
    "sinon-stub-promise": "4.0.0",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "cozy-bar": "5.0.8",
    "cozy-client-js": "0.11.0",
    "cozy-realtime": "1.0.0",
    "cozy-ui": "10.1.2",
    "emoji-js": "3.4.1",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-raven-middleware": "1.2.0",
    "url-search-params-polyfill": "4.0.0"
  }
}
